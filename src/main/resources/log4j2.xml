<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %-5level [%C{1}.%M]: %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="trace" level="trace">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="fatal">
<!--            <AppenderRef ref="Console"/>-->
        </Root>
    </Loggers>
</Configuration>